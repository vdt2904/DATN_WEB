@model DATNWEB.Models.Season
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Blogdetail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="normal-breadcrumb set-bg" data-setbg="@Model.ImageUrl" style="background-image: url('@Model.ImageUrl'); width: 100%; max-height: 300px;">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <div class="normal__breadcrumb__text">
                    <h2>@Model.SeasonName</h2>
                    <p>Welcome to the official Anime blog.</p>
                </div>
            </div>
        </div>
    </div>
</section>
<section class="blog spad">
    <div class="container">
        <div class="row" id="detail">
            
        </div>
    </div>
</section>

<script src="@Url.Content("~/pagesjs/blogdetail.js")"></script>
<script>
    blogdetail('@Model.SeasonId');
</script>